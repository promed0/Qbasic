Print "Enter your exam marks"
Input a
If a > 100 Or a < 0 Then
    Print "This number isn't valid"
Else If a >= 80 Then
        Print "A+"
    Else If a >= 75 Then
            Print "A"
        Else If a >= 70 Then
                Print "A-"
            Else If a >= 65 Then
                    Print "B+"
                Else If a >= 60 Then
                        Print "B"
                    Else If a >= 55 Then
                            Print "B-"
                        Else If a >= 50 Then
                                Print "c+"
                            Else If a >= 45 Then
                                    Print "c"
                                Else If a >= 40 Then
                                        Print "D"
                                    Else
                                        Print "Fail"
                                    End If
                                End If
                            End If
                        End If
                    End If
                End If
            End If
        End If
    End If
End If
